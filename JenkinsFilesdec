node {
def mavenHome = tool name: "maven-1"
//checkout stage
stage{'checkout'){
checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/srj263800-ui/simple-java-maven-app.git']])
{
checkout scm
}
}
//build stage
stage('build'){
sh '"$mavenHome/bin/mvn clean package"'
}

}
